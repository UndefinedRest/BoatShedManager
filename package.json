{
  "name": "lmrc-booking-viewer",
  "version": "3.0.0",
  "description": "LMRC Booking Viewer - fetches and displays 7-day booking view from RevSport",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev:server": "tsx watch src/server/index.ts",
    "build": "tsc && npm run build:frontend",
    "build:frontend": "tsc -p tsconfig.frontend.json",
    "start": "node dist/server/index.js",
    "start:server": "node dist/server/index.js",
    "fetch": "tsx src/index.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "lmrc",
    "booking",
    "rowing",
    "revsport"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "axios-cookiejar-support": "^6.0.4",
    "axios-retry": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.0.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "tough-cookie": "^5.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/tough-cookie": "^4.0.0",
    "@vitest/coverage-v8": "^4.0.10",
    "@vitest/ui": "^4.0.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.10"
  }
}
